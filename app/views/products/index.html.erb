<!--
    . before title? This tells Rails to use a relative locale lookup.
    For .title with the English locale, it will look up en.products.index.title.
-->
<h1><%= t ".title" %></h1>

<%= link_to "New product", new_product_path if authenticated?%>

<div id="products">
  <% @products.each do |product| %>
    <div>
      <%= link_to product.name, product %>
    </div>
  <% end %>
</div>
